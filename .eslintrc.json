{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        },
        "experimentalDecorators": 1
    },
    "rules": {
        "no-console": "off",
        "react/display-name": 0,
        "react/prop-types": 1,
        "no-unused-vars":"off"
    },
    "plugins": ["react"]
}